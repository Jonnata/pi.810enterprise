<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head') %>
<body>
    <main>
      <!-- menu -->
    <%- include('./partials/header') %>
  
    <%- include('./partials/navMenu') %>

    <div class="giftcards-que-amamos">
      <h3>Gift Cards que amamos</h3>
    </div>

    <section class="produtos">
      <% produtos.forEach((produtos) => { %>
      <div class = "produtos-interno">
      <a href="/produtos/<%= produtos.id %>">
        <img src="/images/produtos/<%= produtos.image %>" alt="imagem do produto">
      </a>
      </div>
        <% }) %>
    </section> 
    <%- include('./partials/footer') %>

    </main>
</body>
</html>